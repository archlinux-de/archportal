{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" media="screen" href="{{ asset('build/packagers.css') }}"/>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('build/packagers.js') }}"></script>
{% endblock %}

{% block title %}Packer{% endblock %}

{% block body %}
    <h1 class="mb-4">Packer</h1>
    <table id="packagers" class="table table-striped table-bordered table-sm" width="100%" cellspacing="0"
           data-ajax="{{ path('app_packagers_datatables') }}">
        <thead>
        <tr>
            <th>Name</th>
            <th>E-Mail</th>
            <th>Pakete</th>
            <th>Aktualisierung</th>
        </tr>
        </thead>
    </table>
{% endblock %}
