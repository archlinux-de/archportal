<h3>Aktuelle Neuigkeiten <span class="more">(<a href="{{ news_archive_url }}">mehr</a>)</span></h3><a
        href="{{ path('app_recentnews_index') }}" class="rss-icon"><img src="{{ asset('build/images/rss.png') }}" alt="RSS Feed"/></a>
{% for entry in news_feed %}
    <h4><a href="{{ entry.link }}">{{ entry.title }}</a></h4>
    <p class="date">{{ entry.updated | date('d.m.Y') }}</p>
    {{ entry.summary | raw }}
{% endfor %}
