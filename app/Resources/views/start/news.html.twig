<div class="container-fluid">
        {% for entry in news_feed %}
            <div class="mb-5">
            <div class="d-flex justify-content-between align-items-baseline border border-top-0 border-left-0 border-right-0 mb-2">
                <h2 class="p-0"><a href="{{ entry.link }}">{{ entry.title }}</a></h2>
                <div class="p-0">{{ entry.updated | date('d.m.Y') }}</div>
            </div>

                {{ entry.summary | raw }}
            </div>
        {% endfor %}
</div>
