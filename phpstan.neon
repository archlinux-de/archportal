includes:
	- vendor/phpstan/phpstan-doctrine/extension.neon
	- vendor/phpstan/phpstan-doctrine/rules.neon
	- vendor/phpstan/phpstan-phpunit/extension.neon
	- vendor/phpstan/phpstan-phpunit/rules.neon
	- vendor/phpstan/phpstan-symfony/extension.neon
	- vendor/phpstan/phpstan-deprecation-rules/rules.neon
parameters:
	level: max
	symfony:
		container_xml_path: '%rootDir%/../../../var/cache/dev/App_KernelDevDebugContainer.xml'
	autoload_directories:
		- %rootDir%/../../../src/Migrations
	paths:
		- src
		- tests
		- public
		- config
		- bin
	ignoreErrors:
		-
			message: '#Unsafe usage of new static\(\)\.#'
			path: src/Entity/Packages/Relations/AbstractRelation.php
		-
			message: '#Variable \$candidate in PHPDoc tag @var does not match assigned variable \$candidates\.#'
			path: src/Repository/AbstractRelationRepository.php
		-
			message: '#has no return typehint specified\.#'
			path: tests
		-
			message: '#no value type specified#'
			path: .
